{
  "name": "mem-calculator",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:testing": "npx eas-cli build --profile testing --platform all",
    "build:production": "npx eas-cli build --profile production --platform all",
    "submit": "npx eas-cli submit --profile production --latest --platform all",
    "submit:ios": "npx eas-cli submit --profile production --latest --platform ios",
    "submit:android": "npx eas-cli submit --profile production --latest --platform android",
    "build-ios:testing": "npx eas-cli build --profile testing --platform ios",
    "build-ios:production": "npx eas-cli build --profile production --platform ios",
    "build-android:testing": "npx eas-cli build --profile testing --platform android",
    "build-android:production": "npx eas-cli build --profile production --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/drawer": "^6.5.4",
    "@react-navigation/native": "^6.1.0",
    "expo": "~47.0.8",
    "expo-status-bar": "~1.4.2",
    "native-base": "^3.4.25",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.4.1",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "typescript": "^4.6.3"
  },
  "private": true
}
